{
  "name": "module-develop-template",
  "version": "0.4.6",
  "description": "a module develop boilerplate",
  "keywords": [
    "boilerplate",
    "module",
    "template"
  ],
  "homepage": "https://github.com/arvinxx/module-develop-template",
  "bugs": {
    "url": "https://github.com/arvinxx/module-develop-template"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arvinxx/module-develop-template.git"
  },
  "license": "MIT",
  "author": "arvinxx(arvinx@foxmail.com)",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "clean": "rm -rf es lib dist coverage .dumi/tmp .eslintcache",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && npm run setup",
    "prepublishOnly": "father doctor && npm run build",
    "prettier": "prettier -c --write \"**/**\"",
    "setup": "dumi setup",
    "start": "dumi dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest --update-snapshot",
    "tsc": "tsc -p tsconfig-check.json"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17",
    "@testing-library/react": "^13",
    "@types/jest": "^29",
    "@umijs/max": "^4",
    "antd": "^5",
    "commitlint": "^17",
    "commitlint-config-gitmoji": "^2",
    "concurrently": "^7",
    "cross-env": "^7",
    "dumi": "^2",
    "eslint": "^8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.2.1",
    "father": "^4",
    "husky": "^8",
    "jest": "^29",
    "prettier": "^2",
    "react": "^18",
    "react-dom": "^18",
    "semantic-release": "^19",
    "semantic-release-config-gitmoji": "^1",
    "ts-jest": "^29",
    "typescript": "^4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
